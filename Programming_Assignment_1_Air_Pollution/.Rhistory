both <- colSums(!is.na(read.csv(i))[,2:3])
if (both["sulfate"]<=both["nitrate"]) {result<-append(result,both[["sulfate"]])}
else {result<-append(result,both[["nitrate"]])}
}
data.frame(id=id,nobs=result)
}
